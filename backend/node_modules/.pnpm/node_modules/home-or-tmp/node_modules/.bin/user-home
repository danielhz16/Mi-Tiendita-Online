#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/123/Desktop/MI TIENDITA ONLINE/backend/node_modules/.pnpm/user-home@1.1.1/node_modules/user-home/node_modules:/mnt/c/Users/123/Desktop/MI TIENDITA ONLINE/backend/node_modules/.pnpm/user-home@1.1.1/node_modules:/mnt/c/Users/123/Desktop/MI TIENDITA ONLINE/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/123/Desktop/MI TIENDITA ONLINE/backend/node_modules/.pnpm/user-home@1.1.1/node_modules/user-home/node_modules:/mnt/c/Users/123/Desktop/MI TIENDITA ONLINE/backend/node_modules/.pnpm/user-home@1.1.1/node_modules:/mnt/c/Users/123/Desktop/MI TIENDITA ONLINE/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../user-home@1.1.1/node_modules/user-home/cli.js" "$@"
else
  exec node  "$basedir/../../../../../user-home@1.1.1/node_modules/user-home/cli.js" "$@"
fi
